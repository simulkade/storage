\begin_body

\begin_layout Section
Single phase flow formulation
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial}{\partial t}\left(\varphi\rho\right)+\nabla.\left(\rho\mathbf{u}\right)=0
\]

\end_inset


\begin_inset Formula 
\[
\rho=\rho_{0}+\left(\frac{d\rho}{dp}\right)_{p_{0}}\left(p-p_{0}\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial}{\partial t}\left(\varphi\left(\rho_{0}+\left(\frac{d\rho}{dp}\right)_{p_{0}}\left(p-p_{0}\right)\right)\right)+\nabla.\left(\frac{-k}{\mu}\left(\rho_{0}+\left(\frac{d\rho}{dp}\right)_{p_{0}}\left(p-p_{0}\right)\right)\nabla p\right)=0
\]

\end_inset


\begin_inset Formula 
\[
+\left(\frac{d\rho}{dp}\right)_{p_{0}}\nabla p_{0}\left(p-p_{0}\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{\partial}{\partial t}\left(\varphi\left(\frac{d\rho}{dp}\right)_{p_{0}}p\right)+\nabla.\left(\frac{-k}{\mu}\rho_{0}\nabla p+\left(\frac{-k}{\mu}\left(\frac{d\rho}{dp}\right)_{p_{0}}\nabla p_{0}\right)p\right)=\nabla.\left(\frac{-k}{\mu}\left(\frac{d\rho}{dp}\right)_{p_{0}}p_{0}\nabla p_{0}\right)
\]

\end_inset


\end_layout

